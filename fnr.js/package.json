{
  "name": "fnr.js",
  "version": "2.0.0",
  "description": "Verktøy for å validere fødselsnummer",
  "main": "dist/fnr.js",
  "jsnext:main": "lib/fnr.js",
  "scripts": {
    "build": "babel -d dist/ lib/",
    "lint": "standard",
    "test:readme": "readme-assert",
    "test:spec": "mocha --reporter mocha-tap13",
    "test": "npm run test:spec && npm run test:readme",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "babel-cli": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "chai": "^1.1.2",
    "fnr-testdata": "^1.0.0",
    "mocha": "^2.3.4",
    "mocha-tap13": "0.0.2",
    "readme-assert": "^1.0.0",
    "standard": "^5.4.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/laat/fnr.js.git"
  },
  "author": "Sigurd Fosseng (@fosseng)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/laat/fnr.js/issues"
  },
  "homepage": "https://github.com/laat/fnr.js"
}
