{
  "name": "fnr.js",
  "version": "2.1.0",
  "description": "Verktøy for å validere fødselsnummer",
  "license": "MIT",
  "main": "dist/fnr.js",
  "jsnext:main": "lib/fnr.js",
  "scripts": {
    "build": "babel -d dist/ lib/",
    "lint": "eslint lib",
    "test:readme": "readme-assert --require babel-register --main './lib/fnr.js'",
    "test:spec": "mocha --require babel-register --reporter mocha-tap13",
    "test": "npm-run-all lint test:*",
    "prepublish": "npm run build"
  },
  "author": "Sigurd Fosseng (@fosseng)",
  "repository": {
    "type": "git",
    "url": "https://github.com/laat/fnr.js.git"
  },
  "bugs": {
    "url": "https://github.com/laat/fnr.js/issues"
  },
  "homepage": "https://github.com/laat/fnr.js",
  "devDependencies": {
    "babel-cli": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb-base": "^1.0.3",
    "eslint-plugin-import": "^1.5.0",
    "fnr-testdata": "^1.0.0",
    "mocha": "^2.4.5",
    "mocha-tap13": "^1.0.2",
    "npm-run-all": "^1.8.0",
    "readme-assert": "^4.0.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
