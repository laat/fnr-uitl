{
  "name": "fnr-generator",
  "version": "2.1.0",
  "description": "Verktøy for å generere fødselsnummer",
  "main": "dist/fnr-generator.js",
  "author": "Sigurd Fosseng <sigurd@fosseng.net>",
  "license": "MIT",
  "scripts": {
    "build": "babel -d dist/ fnr-generator.js",
    "lint": "eslint *.js",
    "test:spec": "mocha --require babel-register --reporter mocha-tap13 ",
    "test:readme": "readme-assert --require babel-register --main ./fnr-generator.js",
    "test": "npm-run-all lint test:*",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "fnr.js": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "eslint": "^2.8.0",
    "eslint-config-airbnb-base": "^1.0.3",
    "eslint-plugin-import": "^1.5.0",
    "fnr-testdata": "^1.0.0",
    "mocha": "^2.3.4",
    "mocha-tap13": "^1.0.2",
    "npm-run-all": "^1.8.0",
    "readme-assert": "^4.0.2"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
