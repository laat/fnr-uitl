{
  "name": "fnr-generator",
  "version": "2.0.0",
  "description": "Verktøy for å generere fødselsnummer",
  "main": "dist/fnr-generator.js",
  "jsnext:main": "fnr-generator.js",
  "scripts": {
    "build": "babel -d dist/ fnr-generator.js",
    "lint": "standard",
    "test:spec": "mocha --require babel-register --reporter mocha-tap13 ",
    "test:readme": "readme-assert",
    "test": "npm run test:readme && npm run test:spec",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "fnr.js": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "chai": "^1.1.2",
    "fnr-testdata": "^1.0.0",
    "mocha": "^2.3.4",
    "mocha-tap13": "0.0.2",
    "readme-assert": "^1.0.0",
    "standard": "^5.4.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  },
  "author": "Sigurd Fosseng <sigurd@fosseng.net>",
  "license": "MIT"
}
