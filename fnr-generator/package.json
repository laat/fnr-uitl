{
  "name": "fnr-generator",
  "version": "2.1.1",
  "description": "Verktøy for å generere fødselsnummer",
  "main": "lib/fnr-generator.js",
  "author": "Sigurd Fosseng <sigurd@fosseng.net>",
  "repository": "laat/fnr.js",
  "license": "MIT",
  "scripts": {
    "build": "babel --ignore=test.js -d lib src",
    "lint": "eslint *.js",
    "test:spec": "mocha --require babel-register --reporter mocha-tap13 src/test.js",
    "test:readme": "readme-assert --require babel-register --main ./src/fnr-generator.js",
    "test": "npm-run-all lint test:*",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "fnr.js": "^2.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.7.2",
    "chai": "^3.5.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb-base": "^7.0.0",
    "eslint-plugin-import": "^1.5.0",
    "fnr-testdata": "^1.0.0",
    "mocha": "^2.5.3",
    "mocha-tap13": "^1.0.2",
    "npm-run-all": "^3.1.0",
    "readme-assert": "^5.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "files": [
    "lib"
  ]
}
